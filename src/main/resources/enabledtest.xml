<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <test name="HYRTutorialsTest">
        <classes>
            <class name="com.testng.EnableDisableDemo">
                <methods>
                    <include name="launchBrowser"/>
                    <include name="openApp"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="GoogleFacebookTest">
        <classes>
            <class name="com.testng.FirstTest">
                <methods>
                    <include name="openGoogle"/>
                    <include name="openFacebook"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
